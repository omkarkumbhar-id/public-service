{
  "name": "@nxcontainer/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "serve:service": "nx serve servic",
    "serve:nxcontainer": "nx serve nxcontainer",
    "build": "nx build service && nx build nxcontainer",
    "test": "npm run test:service && npm run test:nxcontainer",
    "test:service": "nx e2e service-e2e",
    "test:nxcontainer": "nx e2e nxcontainer-e2e",
    "build:images": "npm run build && docker build -t experiment-service -f apps/service/Dockerfile . && docker build -t experiment-nxcontainer -f apps/nxcontainer/Dockerfile .",
    "images:start": "docker run -p 3001:3001 --name experiment-service -d experiment-service && docker run -p 3000:3000 --name experiment-nxcontainer -d experiment-nxcontainer",
    "images:stop": "docker stop experiment-service && docker stop experiment-nxcontainer",
    "compose:up": "npm run build && docker compose up --build",
    "compose:down": "docker compose down"
  },
  "private": true,
  "dependencies": {
    "axios": "^1.6.0",
    "express": "~4.18.1",
    "next": "14.2.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
